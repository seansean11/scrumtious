{
  "name": "scrumtious",
  "version": "1.0.0",
  "description": "An app for organizing your agile Atlassian development",
  "main": "index.js",
  "scripts": {
    "start": "ENV=development electron .",
    "dev": "concurrently -k 'babel-node server.js' 'npm start'",
    "test": "ava \"src/**/*.test.js\" --verbose",
    "watch:test": "npm run test -- --watch",
    "cover": "nyc npm run test",
    "build": "webpack --config webpack.prod.js && electron-packager . Scrumtious --platform=darwin --arch=all --overwrite"
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/setup.js"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/electron/electron-quick-start.git"
  },
  "keywords": [
    "agile",
    "productivity",
    "Atlassian"
  ],
  "license": "MIT",
  "author": "Sean Michael",
  "homepage": "https://seanmichael.me",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^2.0.3",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.16.3",
    "concurrently": "^2.2.0",
    "css-loader": "^0.23.1",
    "electron-devtools-installer": "^2.0.0",
    "electron-packager": "^7.2.0",
    "electron-prebuilt": "^1.2.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "nyc": "^8.3.2",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.3.2",
    "redux-devtools": "^3.3.1",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "electron-compile": "^3.3.5",
    "electron-debug": "^1.0.1",
    "electron-prebuilt": "^1.4.3",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "lodash": "^4.16.4",
    "marked": "^0.3.5",
    "material-design-icons": "^2.2.3",
    "react": "^15.2.1",
    "react-contenteditable": "^2.0.1",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-reorder": "^2.1.0",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "request": "^2.65.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  }
}
